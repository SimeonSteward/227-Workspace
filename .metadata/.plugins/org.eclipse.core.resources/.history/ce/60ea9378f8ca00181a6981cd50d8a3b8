package mini1;

public class KeepMeInTheLoop {

	public static int findNth(String s, char ch, int n) {
		int b=0;
		for (int i = 0; i < s.length()-1; i++) {
			if (s.charAt(i)==ch) {
				b++;
				if (b == n) {
					return i;
				}
			}
		}
		return -1;
		// TODO
	}

